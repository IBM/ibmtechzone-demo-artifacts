      "#Income",
      "#Rate",
      "#Age"
    ]
  ],
  "action": {
    "name": "Transform",
    "subaction": {
      "name": "redactColumns",
      "parameters": [
        {
          "name": "column_names",
          "value": [
            "Income",
            "Rate",
            "Age"
          ]
        }
      ]
    }
  },
  "state": "active"
},
